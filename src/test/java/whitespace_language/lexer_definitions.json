{
  "tokens": [
    {
      "name": "whitespace",
      "pattern": "( |\t)+"
    },
    {
      "name": "newline",
      "pattern": "\\r\\n|\\r|\\n",
      "keepInAST": false
    },
    {
      "name": "identifier",
      "pattern": "[a-zA-Z_][a-zA-Z_0-9]*"
    }
  ],
  "postProcessors": ["keepWhitespacesSOL"]
}